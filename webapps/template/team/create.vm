<form name="teamForm" action="$!homeServer.getURI("team/doCreate.htm")" method="post">
  <table class="table">
    #springBind("teamForm.jerseyColor")
    <tr>
        <td>球服颜色</td>
        <td>
            <input class="form-control" type="text" name="${status.expression}" value="$!status.value" />
            #foreach($error in $status.errorMessages) <b>$error</b>  #end
        </td>
    </tr>
    #springBind("teamForm.homeCourt")
    <tr>
        <td>主场</td>
        <td>
            <input class="form-control" type="text" name="${status.expression}" value="$!status.value" />
            #foreach($error in $status.errorMessages) <b>$error</b>  #end
        </td>
    </tr>
    #springBind("teamForm.area")
    <tr>
        <td>球队活动区域</td>
        <td>
            <input class="form-control" type="text" name="${status.expression}" value="$!status.value" />
            #foreach($error in $status.errorMessages) <b>$error</b>  #end
        </td>
    </tr>
    #springBind("teamForm.canJoin")
    <tr>
        <td>是否开放加入</td>
        <td>
            <input class="form-control" type="text" name="${status.expression}" value="$!status.value" />
            #foreach($error in $status.errorMessages) <b>$error</b>  #end
        </td>
    </tr>
    #springBind("teamForm.teamType")
    <tr>
        <td>球队类型</td>
        <td>
            <input class="form-control" type="text" name="${status.expression}" value="$!status.value" />
            #foreach($error in $status.errorMessages) <b>$error</b>  #end
        </td>
    </tr>
    #springBind("teamForm.teamDesc")
    <tr>
        <td>球队口号</td>
        <td>
            <input class="form-control" type="text" name="${status.expression}" value="$!status.value" />
